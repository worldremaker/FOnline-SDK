#include "_macros.fos"

#define STR_USE_MONUMENT    ( 70060 )

bool s_Monument( Critter& cr, Scenery& terminal, int skill, Item@ item )
{
    if( skill == -1 && not valid( item ) )
    {
        cr.SayMsg( SAY_NETMSG, TEXTMSG_TEXT, STR_USE_MONUMENT );
    }
    return false;
}
